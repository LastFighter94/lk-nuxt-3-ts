{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "version": "2.21.04",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "type-check": "nuxi typecheck --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "storybook": "storybook dev --port 6006",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxtjs/device": "^3.2.4",
    "@nuxtjs/storybook": "^8.4.1",
    "@pinia/nuxt": "^0.11.1",
    "@storybook-vue/nuxt": "^8.4.1",
    "@vue/eslint-config-airbnb": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.1.7",
    "js-sha256": "^0.9.0",
    "lint-staged": "^15.2.2",
    "lodash": "^4.17.21",
    "maska": "^3.0.0",
    "nuxt": "^3.17.6",
    "pinia": "^3.0.3",
    "prettier": "^3.2.5",
    "storybook": "^9.0.5",
    "swiper": "^11.2.10",
    "uuid": "^11.1.0",
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "axios": "^1.12.2",
    "sass": "^1.89.2",
    "typescript": "^5.8.3",
    "vue-tsc": "^2.2.12"
  },
  "lint-staged": {
    "**/*.{vue,ts}": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  }
}
